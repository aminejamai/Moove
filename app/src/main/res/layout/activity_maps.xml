<?xml version="1.0" encoding="utf-8"?>


<LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto">


    <fragment
            android:id="@+id/startExercice"
            android:name="com.example.moove.startExerciceFragment"
            android:layout_width="match_parent"
            android:layout_height="379dp"
            android:layout_weight="1"

            tools:layout_editor_absoluteX="1dp" app:layout_constraintBottom_toTopOf="@+id/map"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.0"/>

    <fragment
            android:layout_width="416dp"
            android:layout_height="595dp"
            android:id="@+id/map"
            tools:context=".MapsActivity"
            android:name="com.google.android.gms.maps.SupportMapFragment" tools:layout_editor_absoluteY="252dp"
            tools:layout_editor_absoluteX="-6dp"/>

</LinearLayout>


